package JavaProgrammingInternalTCS;

public class FindrepetativeNum {
	
	public void FindRN() {
		
		String Str="Vijaykumar";
		
		Str=Str.toLowerCase();
		//int i =0;
		for(int i=0;i<Str.length();i++);{
			
		
		for(int j=i+1;j<=Str.length();j++);{
			
			if(Str.charAt(i)==Str.charAt(j))
			{
				System.out.println(Str.charAt(i));
			}
		}
		}
		
	}

}
